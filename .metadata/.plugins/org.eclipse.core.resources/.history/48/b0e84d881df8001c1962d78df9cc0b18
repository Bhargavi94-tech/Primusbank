public class AdminHomePage {
	
	@FindBy(xpath="//img[@src='images/admin_but_03.jpg']")
	private WebElement logout;
	WebDriverWait wait= new WebDriverWait(driver,20);
	
	public void logout() {
		
		logout.click();
	}

}
